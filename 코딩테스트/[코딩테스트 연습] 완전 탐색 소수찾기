package main;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Main {
	public static void main(String[]args) {
		String numbers="127";
		String tempNumbers[] = numbers.split("");
		List<String> arr = Arrays.asList(tempNumbers);
		ArrayList<String> returnArr = new ArrayList<>();

		result(arr,returnArr,"");
		System.out.println(returnArr);
	}
	
	
	public static ArrayList<String> result(List<String> arr, ArrayList<String> returnArr,String temp){
		for(int i=0;i<arr.size();i++) {
			//System.out.println(arr.get(i));
			if(temp.length()<arr.size()) {
				
				temp+=arr.get(i);
				returnArr.add(temp);
				
				List<String> tempArr = new ArrayList<String>(arr);
				tempArr.remove(i);
				//2,3
				result(tempArr,returnArr,temp);
			}else {
				temp+=arr.get(i);
				returnArr.add(temp);
				temp="";
			}

			temp="";
		}

		return returnArr;
		
	}
	

}
